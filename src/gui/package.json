{
  "name": "LC3Tools",
  "version": "2.0.3",
  "author": "Chirag Sakhuja <chirag.sakhuja@utexas.edu>",
  "description": "LC3Tools frontend",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chiragsakhuja/lc3tools.git"
  },
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "lc3": "yarn add file:./local_modules/lc3interface",
    "release": "node .electron-vue/build.js && electron-builder --publish always"
  },
  "extraResources": "",
  "buildResources": "static",
  "build": {
    "productName": "LC3Tools",
    "appId": "org.devtools.lc3tools",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*",
      "static/icons/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "static/icons/icon.icns",
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "win": {
      "icon": "static/icons/icon.ico",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "icon": "static/icons",
      "category": "Development",
      "target": "AppImage"
    },
    "publish": {
      "provider": "github",
      "owner": "chiragsakhuja",
      "repo": "lc3tools"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "electron-log": "^4.4.8",
    "electron-storage": "^1.0.7",
    "electron-updater": "^6.1.4",
    "lc3interface": "file:./local_modules/lc3interface",
    "material-design-icons-iconfont": "^3.0.3",
    "vue": "^2.6.12",
    "vue-electron": "^1.0.6",
    "vue-electron-storage": "^1.0.1",
    "vue-router": "^3.4.9",
    "vue2-ace-editor-electron": "^0.0.4",
    "vuetify": "^1.5.24",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/register": "^7.22.15",
    "babel-loader": "^9.1.3",
    "cfonts": "^3.2.0",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "del": "^7.1.0",
    "devtron": "^1.4.0",
    "electron": "^26.2.4",
    "electron-builder": "^24.6.4",
    "electron-debug": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.8.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.88.2",
    "webpack-dev-server": "^4.15.1"
  }
}
